##navigation
alias ..='cd ..'
alias ...='cd ../..'
alias ll='ls -l'
alias lla='ls -la'

##system handling
#alias 'updade'='sudo apt-get update && sudo apt-get upgrade -y'
alias update-alias='source ~/.bash_aliases'


##connections
alias pi2conn='ssh pi@rpi2'
alias pi3conn='ssh pi@rpi3'

##Docker commands
alias dcls="docker container ls -a"
alias dils="docker image ls"
alias dnls="docker network ls"
alias dvls="docker volume ls"

